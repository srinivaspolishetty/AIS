<div class="row p-t-1 p-a-0 m-a-0">
  <div class="col-md-8">
    <h4>Command List | <span class="">Total Records : {{page.totalElements}}</span> </h4>
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
          <li class="breadcrumb-item"><a [routerLink]="['/dashboard']">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">Command Management</li>
          <li class="breadcrumb-item active" aria-current="page">Command List</li>
      </ol>
    </nav>
  </div>
  <div class="col-md-4 text-right">
    <button type="button" class="btn-search" (click)="btnShowFilters()"><i class="icon-ic_searchfilter"></i></button>
  </div>
</div>


<div class="row p-a-0 m-a-0 page-filters" [ngClass]="{'show': showFilters == true}">
  <div class="col-md-12">
    <div class="row m-a-0 p-a-0">
        <div class="col-md-4  p-r-0"></div>
      <div class="col-md-4  p-r-0">
        <div class="form-group">
          <input type="text" class="form-control" placeholder="Search By IMEI No / Vehicle No"
          [(ngModel)]="searchValue" (keyup)="searchData();">
          <button type="button" class="btn-close close" aria-label="Close">
            <span aria-hidden="true" (click)="clearSearch();" *ngIf="showSearchClear">&times;</span>
          </button>
        </div>
      </div>
      <div class="col-md-4 p-r-0">
        <div class="form-group">
          <div class="input-group-append datepicker">
            <input type="text" class="form-control" readOnly placeholder="yyyy-mm-dd" name="dp"
              [(ngModel)]="model" (dateSelect)="changeDate();" ngbDatepicker #d="ngbDatepicker">
            <button style="outline:none" class="date_btn_style calendar" (click)="d.toggle()" type="button"> <i class="fa fa-calendar"
                aria-hidden="true"></i></button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row p-a-0 m-a-0">
  <div class="col-md-12">
    <div class="box-rect-round">
      <table class="grid-table m-t-3">
        <thead>
          <tr>
            <th>IMEI No</th>
            <th>Task ID</th>
            <th>Header</th>
            <th>Random Code</th>
            <th width="150px">Reply Gateway No</th>
            <th width="250px">Payload</th>
          </tr>
        </thead>
        <tbody>
          <tr *ngFor="let list of commandListData | paginate: { id: 'commandlistpage', itemsPerPage: page.size, currentPage: p, totalItems: page.totalElements }">
            <td>{{list.imeiNumber}}</td>
            <td>{{list.taskId}}</td>
            <td>{{list.header}}</td>
            <td>{{list.randomCode}}</td>
            <td>{{list.replyGatewayNumber}}</td>
            <td>{{list.payload}}</td>
          </tr>
        </tbody>
      </table>
      <div class="row">
        <div style="margin:0px auto;">
          <pagination-controls id="commandlistpage" class="pagination" (pageChange)="pageChanged($event)" maxSize="6"
            directionLinks="true" autoHide="true" responsive="true" previousLabel="Prev" nextLabel="Next">
          </pagination-controls>
        </div>
      </div>
    </div>
  </div>
</div>