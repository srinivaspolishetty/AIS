<div class="modal-header">
  <h5 class="modal-title">Device Details</h5>
  <button type="button" class="close" aria-label="Close" (click)="activeModal.close('Close click')">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<div class="modal-body">
  <div class="row">
    <div class="col-md-6">
      <h5>ID</h5>
      <h4>{{selectedDeviceDetails.modemDeviceId}}</h4>
    </div>
    <div class="col-md-6">
      <h5>IMEI No</h5>
      <h4>{{selectedDeviceDetails.imeiNumber}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>IMSI No</h5>
      <h4>{{selectedDeviceDetails.imsiNumber}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Sim No</h5>
      <h4>{{selectedDeviceDetails.simNumber}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>IP Address</h5>
      <h4>{{selectedDeviceDetails.ipAddress}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Signal Strength</h5>
      <h4>{{selectedDeviceDetails.signalStrength}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Version</h5>
      <h4>{{selectedDeviceDetails.version}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Mobile No</h5>
      <h4>{{selectedDeviceDetails.mobileNumber}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Created Date</h5>
      <h4>{{selectedDeviceDetails.createdDate}}</h4>
    </div>
  </div>
</div>
