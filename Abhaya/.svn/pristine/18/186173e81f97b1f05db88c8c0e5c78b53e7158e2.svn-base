<div class="modal-header">
  <h5 class="modal-title">Device Communication Details | <span> Last Packet : "{{selectedComDetails.networkDate}} {{selectedComDetails.networkTime}}"</span> </h5>
  <button type="button" class="close" aria-label="Close" (click)="activeModal.close('Close click')">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<div class="modal-body">
  <div class="row">
    <div class="col-md-6">
      <h5>ID</h5>
      <h4>{{selectedComDetails.deviceCommunicationId}}</h4>
    </div>
    <div class="col-md-6">
      <h5>IMEI No</h5>
      <h4>{{selectedComDetails.imeiNumber}}</h4>
    </div>
  </div>
  
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Longitude</h5>
      <h4>{{selectedComDetails.langitude}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Latitude</h5>
      <h4>{{selectedComDetails.latitude}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Engine Status</h5>
      <h4>{{selectedComDetails.ignitionStatus}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Ignition Status</h5>
      <h4>{{selectedComDetails.ignitionStatus}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Network Date</h5>
      <h4>{{selectedComDetails.networkDate}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Network Time</h5>
      <h4>{{selectedComDetails.networkTime}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Prev Langitude</h5>
      <h4>{{selectedComDetails.prevLangitude}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Prev Latitude</h5>
      <h4>{{selectedComDetails.prevLatitude}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Created Date</h5>
      <h4>{{selectedComDetails.createdDate}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Location</h5>
      <h4>{{selectedComDetails.location}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Prev Network Date</h5>
      <h4>{{selectedComDetails.prevNetworkDate}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Prev Network Time</h5>
      <h4>{{selectedComDetails.prevNetworkTime}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Panic Status</h5>
      <h4>{{selectedComDetails.panicButtonStatus}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Tamper Status</h5>
      <h4>{{selectedComDetails.tamperAlert}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Status</h5>
      <h4>{{selectedComDetails.status}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
      <div class="col-md-6">
        <h5>Is Over Speed</h5>
        <h4>{{selectedComDetails.isDeviceOverSpeed}}</h4>
      </div>
    </div>
</div>