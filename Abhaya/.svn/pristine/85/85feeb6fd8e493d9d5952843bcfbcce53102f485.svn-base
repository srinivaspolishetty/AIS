      </div>
      <div class="row p-a-0 m-a-0">
            <div class="col-md-12">
<div class="box-rect-round">
  <table class="abhaya-table m-t-3">
    <thead>
      <tr>
        <th>ID</th>
        <th>User Name</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <tr *ngFor="let user of userdata">
        <td>{{user.userId}}</td>
        <td>{{user.username}}</td>
        <td>{{user.firstName}}</td>
        <td>{{user.lastName}}</td>
        <td>{{user.emailId}}</td>
        <td><i class="fa fa-ellipsis-v" aria-hidden="true"></i></td>
      </tr>
    </tbody>
  </table>
      </div>
    </div>
      </div>