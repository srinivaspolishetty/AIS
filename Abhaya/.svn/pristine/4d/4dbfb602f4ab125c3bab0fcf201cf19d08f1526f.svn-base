<div class="modal-header">
  <h5 class="modal-title">Health Monitoring Details</h5>
  <button type="button" class="close" aria-label="Close" (click)="activeModal.close('Close click')">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<div class="modal-body">
  <div class="row">
    <div class="col-md-6">
      <h5>ID</h5>
      <h4>{{selectedComDetails.deviceCommunicationId}}</h4>
    </div>
    <div class="col-md-6">
      <h5>IMEI No</h5>
      <h4>{{selectedComDetails.imeiNumber}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Serial Number</h5>
      <h4>{{selectedComDetails.serialNumber}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Status</h5>
      <h4>{{selectedComDetails.status}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Longitude</h5>
      <h4>{{selectedComDetails.langitude}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Latitude</h5>
      <h4>{{selectedComDetails.latitude}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Engine Status</h5>
      <h4>{{selectedComDetails.engineStatus}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Ignition Status</h5>
      <h4>{{selectedComDetails.ignitionStatus}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Packet Date</h5>
      <h4>{{selectedComDetails.packetDate}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Packet Time</h5>
      <h4>{{selectedComDetails.packetTime}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Prev Langitude</h5>
      <h4>{{selectedComDetails.prevLangitude}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Prev Latitude</h5>
      <h4>{{selectedComDetails.prevLatitude}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Created Date</h5>
      <h4>{{selectedComDetails.createdDate}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Location</h5>
      <h4>{{selectedComDetails.location}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Prev Packet Date</h5>
      <h4>{{selectedComDetails.prevPacketDate}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Prev Packet Time</h5>
      <h4>{{selectedComDetails.prevPacketTime}}</h4>
    </div>
  </div>
  <div class="row p-t-2">
    <div class="col-md-6">
      <h5>Panic Status</h5>
      <h4>{{selectedComDetails.panicButtonStatus}}</h4>
    </div>
    <div class="col-md-6">
      <h5>Temperature Status</h5>
      <h4>{{selectedComDetails.tamperStatus}}</h4>
    </div>
  </div>
</div>
