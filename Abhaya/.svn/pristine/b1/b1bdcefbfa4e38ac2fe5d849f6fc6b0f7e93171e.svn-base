<div class="row p-t-1 p-a-0 m-a-0">
  <div class="col-md-10">
    <h4>Emergency Management</h4>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="#">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">Emergency Management</li>
        </ol>
      </nav>
  </div>
  <!-- <div class="col-md-2 text-right">
    <button type="button" class="active btn-left btn-toggle">Today</button>
    <button type="button" class="btn-right btn-toggle">All</button>
  </div> -->
  
</div>
<div class="row p-a-0 m-a-0">
  <div class="col-md-12">
    <div class="box-rect-round">
      <table class="abhaya-table m-t-3">
        <thead>
          <tr>
            <th>ID</th>
            <th>IMEI No</th>
            <th>Vehicle No</th>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Packet Type</th>
            <th>Reply No</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <tr *ngFor="let details of emergencyMonitoringdata">
            <td>{{details.ebpPacketDataId}}</td>
            <td>{{details.imeiNumber}}</td>
            <td>{{details.vehicleRegNo}}</td>
            <td>{{details.latitude}}</td>
            <td>{{details.longitude}}</td>
            <td>{{details.packetType}}</td>
            <td>{{details.replyNumber}}</td>
            <td><i class="fa fa-ellipsis-v" aria-hidden="true"></i></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>