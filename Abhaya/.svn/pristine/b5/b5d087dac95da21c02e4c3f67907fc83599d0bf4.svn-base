<div class="row p-t-1 ">
    <div class="col-md-10">
      <h4>Create Trip</h4>
      <nav aria-label="breadcrumb" class="d-none d-sm-block">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a [routerLink]="['/dashboard']">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">Create Trip </li>
        </ol>
      </nav>
    </div>
  </div>
  <div class="row">
      <div class="col-md-12">
        <div class="box-rect-round">
          <form>
            <div class="row p-t-6 p-r-4 m-a-0">
              <div class="col-md-4 p-r-0 form-group">
                <label>Execute Type</label>
                <select class="form-control">
                  <option>Select</option>
                  <option>SET</option>
                  <option>GET</option>
                </select>
           
              </div>
             
              <div class="col-md-4 p-r-0 form-group">
                <label>IMEI Number</label>
                <select class="form-control">
                  <option>Select IMEI Number</option>
                  <option>device</option>
                </select>
            
              </div>
              <div class="col-md-4 p-r-0 form-group">
                <label>Value</label>
                <input type="text" class="form-control">
              
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>